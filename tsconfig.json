{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./build",
    "rootDir": "./src"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}